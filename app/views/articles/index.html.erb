<h1>Articles listing page</h1>
<table>
    <thead>
        <tr>
            <th>Title</th>
            <th>Content</th>
            <th colspan="2"> Actions </th>
        </tr>
    </thead>
    <tbody>
        <% @articles.each do |article| %>
            <tr>
                <td><%= article.title %></td>
                <td><%= article.content %></td>
                <td><%= link_to 'Show', article_path(article) %></td>
                <td><%= link_to 'Edit', edit_article_path(article) %></td>
            </tr>
        <% end %>
    </tbody>